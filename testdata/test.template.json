{
    "manifestVersion": "0.0.3",
    "job": {
        "name": "$NAME",
        "algorithmVersion": "$VERSION",
        "packageVersion": "1.0.0",
        "title": "$NAME",
        "description": "Runs the $NAME algorithm",
        "tag": [
        ],
        "authorName": "$AUTHORNAME",
        "authorEmail": "",
        "authorUrl": "",
        "timeout": 3600,
        "cpus": 1.0,
        "mem": 1024.0,
        "sharedMem": 0.0,
        "storage": 0.0,
        "interface": {
            "args": "${INPUT_FILE} ${JOB_OUTPUT_DIR}",
            "inputData": {
                "files": [
                    {
                        "name": "INPUT_FILE",
                        "required": true,
                        "mediaType": [
                            "image/x-hdf5-image"
                        ]
                    }
                ]
            },
            "outputData": {
                "files": [
                    {
                        "name": "output_file_tiffs",
                        "mediaType": "image/tiff",
                        "pattern": "outfile*.tif"
                    }
                ]
            }$ENV_VAR_SECTION$SETTING_SECTION$MOUNT_SECTION
        }$ERROR_SECTION
    }
}
