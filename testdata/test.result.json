{
    "manifestVersion": "0.0.3",
    "job": {
        "name": "my-name",
        "algorithmVersion": "1.2.3",
        "packageVersion": "1.0.0",
        "title": "my-name",
        "description": "Runs the my-name algorithm",
        "tag": [
        ],
        "authorName": "jtobe",
        "authorEmail": "",
        "authorUrl": "",
        "timeout": 3600,
        "cpus": 1.0,
        "mem": 1024.0,
        "sharedMem": 0.0,
        "storage": 0.0,
        "interface": {
            "args": "${INPUT_FILE} ${JOB_OUTPUT_DIR}",
            "inputData": {
                "files": [
                    {
                        "name": "INPUT_FILE",
                        "required": true,
                        "mediaType": [
                            "image/x-hdf5-image"
                        ]
                    }
                ]
            },
            "outputData": {
                "files": [
                    {
                        "name": "output_file_tiffs",
                        "mediaType": "image/tiff",
                        "pattern": "outfile*.tif"
                    }
                ]
            },
            "envVars": [
                {
                    "name": "A",
                    "value": "${SETTING0}"
                },
                {
                    "name": "B",
                    "value": "${SETTING1}"
                },
                {
                    "name": "C",
                    "value": "${SETTING2}"
                }
            ],
            "settings": [
                {
                    "name": "D",
                    "secret": false
                },
                {
                    "name": "E",
                    "secret": false
                },
                {
                    "name": "SETTING0",
                    "secret": false
                },
                {
                    "name": "SETTING1",
                    "secret": false
                },
                {
                    "name": "SETTING2",
                    "secret": false
                }
            ],
            "mounts": [
                {
                    "name": "MOUNT0",
                    "path": "/app/home",
                    "mode": "ro"
                }

            ]

        },
        "errorMapping": [
                
            {
                "code": 10,
                "title": "Error 10",
                "description": "Generic Error Description",
                "category": "algorithm"
            },
            {
                "code": 20,
                "title": "Error 20",
                "description": "Generic Error Description",
                "category": "algorithm"
            },
            {
                "code": 30,
                "title": "Error 30",
                "description": "Generic Error Description",
                "category": "algorithm"
            }

        ]

    }
}
